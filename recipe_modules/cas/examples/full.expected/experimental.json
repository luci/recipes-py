    ],
    "name": "touch a"
  },
  {
    "cmd": [
      "touch",
      "[CLEANUP]/cas-example_tmp_1/b"
    ],
    "name": "touch b"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "ensure-directory",
      "--mode",
      "0777",
      "[CLEANUP]/cas-example_tmp_1/sub/dir"
    ],
    "infra_step": true,
    "name": "mkdirs"
  },
  {
    "cmd": [
      "touch",
      "[CLEANUP]/cas-example_tmp_1/sub/dir/d"
