[
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "Here is some text data",
      "[START_DIR]/some file"
    ],
    "infra_step": true,
    "name": "write a file",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@some file@Here is some text data@@@",
      "@@@STEP_LOG_END@some file@@@"
    ]
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "symlink",
      "[START_DIR]/some file",
      "[START_DIR]/new path"
    ],
    "infra_step": true,
    "name": "symlink it"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/new path",
      "/path/to/tmp/"
    ],
    "infra_step": true,
    "name": "read it"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/symlink.py",
      "--link-json",
      "{\"[START_DIR]/a-second-file\": [\"[CLEANUP]/root/yet/another/symlink\"], \"[START_DIR]/some file\": [\"[CLEANUP]/root/another/symlink\"]}"
    ],
    "infra_step": true,
    "name": "create a tree of symlinks"
  },
  {
    "name": "$result"
  }
]