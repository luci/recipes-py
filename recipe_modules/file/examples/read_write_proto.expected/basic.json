[
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "fields: \"abc\"\nfields: \"def\"\n",
      "[START_DIR]/message.textproto"
    ],
    "infra_step": true,
    "name": "write_proto",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@message.textproto@fields: \"abc\"@@@",
      "@@@STEP_LOG_LINE@message.textproto@fields: \"def\"@@@",
      "@@@STEP_LOG_END@message.textproto@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/message.textproto",
      "/path/to/tmp/tpb"
    ],
    "infra_step": true,
    "name": "read_proto",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@proto.output@{@@@",
      "@@@STEP_LOG_LINE@proto.output@  \"fields\": [@@@",
      "@@@STEP_LOG_LINE@proto.output@    \"abc\", @@@",
      "@@@STEP_LOG_LINE@proto.output@    \"def\"@@@",
      "@@@STEP_LOG_LINE@proto.output@  ]@@@",
      "@@@STEP_LOG_LINE@proto.output@}@@@",
      "@@@STEP_LOG_END@proto.output@@@",
      "@@@STEP_LOG_LINE@message.textproto@fields: \"abc\"@@@",
      "@@@STEP_LOG_LINE@message.textproto@fields: \"def\"@@@",
      "@@@STEP_LOG_END@message.textproto@@@"
    ]
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "RECIPE_MODULE[recipe_engine::file]/resources/fileutil.py",
      "--json-output",
      "/path/to/tmp/json",
      "copy",
      "[START_DIR]/message.textproto",
      "/path/to/tmp/tpb"
    ],
    "infra_step": true,
    "name": "read_proto_again",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@proto.output@{}@@@",
      "@@@STEP_LOG_END@proto.output@@@",
      "@@@STEP_LOG_LINE@message.textproto@@@@",
      "@@@STEP_LOG_END@message.textproto@@@"
    ]
  },
  {
    "name": "$result"
  }
]