[
  {
    "cmd": [
      "cipd",
      "ensure",
      "-root",
      "[START_DIR]/reporter",
      "-ensure-file",
      "infra/tools/security/provenance_broker/${platform} git_revision:3d14b689ac33c182daa4c0602819ede40bb4e128",
      "-max-threads",
      "0",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "name": "ensure_installed",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"result\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      {@@@",
      "@@@STEP_LOG_LINE@json.output@        \"instance_id\": \"resolved-instance_id-of-git_revision:3d1\",@@@",
      "@@@STEP_LOG_LINE@json.output@        \"package\": \"infra/tools/security/provenance_broker/resolved-platform\"@@@",
      "@@@STEP_LOG_LINE@json.output@      }@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "[START_DIR]/reporter/snoopy_broker",
      "-report-stage",
      "-stage",
      "start",
      "-recipe",
      "bcid_reporter:examples/usage",
      "-pid",
      "54321"
    ],
    "name": "snoop: report_stage"
  },
  {
    "cmd": [
      "[START_DIR]/reporter/snoopy_broker",
      "-report-stage",
      "-stage",
      "fetch",
      "-backend-url",
      "http://test.local"
    ],
    "name": "snoop: report_stage (2)"
  },
  {
    "cmd": [
      "[START_DIR]/reporter/snoopy_broker",
      "-report-cipd",
      "-digest",
      "deadbeef",
      "-pkg-name",
      "example/cipd/package",
      "-iid",
      "fakeiid",
      "-backend-url",
      "http://test.local"
    ],
    "name": "snoop: report_cipd"
  },
  {
    "cmd": [
      "[START_DIR]/reporter/snoopy_broker",
      "-report-gcs",
      "-digest",
      "deadbeef",
      "-gcs-uri",
      "gs://bucket/path/to/binary",
      "-backend-url",
      "http://test.local"
    ],
    "name": "snoop: report_gcs"
  },
  {
    "cmd": [
      "[START_DIR]/reporter/snoopy_broker",
      "-report-gcs",
      "-digest",
      "deadbeef",
      "-gcs-uri",
      "gs://bucket/path/to/binary.spdx.jsonl",
      "-sbom-subject",
      "beefdead",
      "-backend-url",
      "http://test.local"
    ],
    "name": "snoop: report_sbom"
  },
  {
    "name": "$result"
  }
]