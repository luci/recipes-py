# vpython VirtualEnv installation package for Recipe Engine.

python_version: "2.7"

wheel: <
  name: "infra/python/wheels/coverage/${vpython_platform}"
  version: "version:4.5.3"
>

# Argparse is in python stdlib... but! Old versions of python 2.7 have versions
# of this with bugs. This version is bug-free (well... it doesn't have any bugs
# that we care about).
wheel: <
  name: "infra/python/wheels/argparse-py2_py3"
  version: "version:1.4.0"
>

##
# BEGIN "cryptography" dependencies.
##

wheel: <
  name: "infra/python/wheels/cryptography/${vpython_platform}"
  version: "version:2.0.3"
>

